{
  "version": "v1beta2",
  "mappings": {
    "_meta": {
      "type": "grafeas"
    },
    "properties": {
      "build": {
        "type": "object",
        "properties": {
          "provenance": {
            "type": "object",
            "properties": {
              "builtArtifacts": {
                "type": "nested",
                "properties": {
                  "checksum": {
                    "type": "keyword"
                  },
                  "id": {
                    "type": "keyword"
                  },
                  "names": {
                    "type": "keyword"
                  }
                }
              }
            }
          }
        }
      }
    },
    "dynamic_templates": [
      {
        "strings_as_keywords": {
          "match_mapping_type": "string",
          "mapping": {
            "type": "keyword",
            "norms": false
          }
        }
      }
    ]
  }
}
